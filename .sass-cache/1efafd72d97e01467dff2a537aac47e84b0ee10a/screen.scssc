// Imports -----------------------------------------------------------------------

@import "compass/reset";
@import "compass/css3/font-face";
@import "compass/css3/background-size";
@import "compass/css3/border-radius";
@import "compass/css3/images";
@import "compass/css3/transform";
@import "compass/css3/transition";
@import "compass/css3/box-shadow";
// @import "animate";
@import "pbj-home";
@import "base";


// Global -----------------------------------------------------------------------

html, body{
  position: relative; /* this seemed to fix the bug where chrome wouldn't show text after making an element absolutely positioned */
}

p {
  margin: 1em 0;
}

em {
  font-family: Georgia, sans-serif;
  font-weight: normal;
  font-style: italic;
}

span.amp {
  font-family: 'Open Sans', sans-serif;
  font-style: normal;
  font-weight: normal;
  }


// Links -----------------------------------------------------------------------

a {
  color: $link-text-color;
  // background: $link-bg-color;
  text-decoration: none;
  padding: 0 0.2em;
  // border-radius: 0.3em;
  border-bottom: dotted 1px $link-text-color;

  &:hover, a:focus {
    color: white;
    background: $link-text-color;
    border-radius: 0.2em;
  }
}

.roll-link {
  display: inline-block;
  overflow: hidden;
  vertical-align: top;
  @include perspective(600px);
  @include perspective-origin(50% 50%);
}

.roll-link:hover {
  text-decoration: none;
}

.roll-link a {
  @include unpink;
  display: block;
  position: relative;
  padding: 0 0.5em;
  @include transition(all 400ms ease);
  @include transform-origin(50%, 0%);
  @include transform-style(preserve-3d);
}

.roll-link:hover a {
  background: $main-bg-color;
  @include translate3d(0, 0, -30px);
  @include rotateX(90deg);
}

.roll-link a:after {
  content: attr(data-title);
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  padding: 0 0.5em;
  color: $main-text-color;
  background: $main-bg-color;
  @include transform-origin(50%, 0%);
  -webkit-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  -moz-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  -ms-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  // tried to use compass mixins for these transforms, but couldn't figure out the right stuff for "create-transform" and the others can't be used together
  // @include translate3d(0px, 105%, 0px);
  // @include rotateX(-90deg);
  // @include create-transform(false, false, false, false, -90deg, false, false, false, 0px, 105%, 0px, false, false, 50%, 0%);
}

// Layout -----------------------------------------------------------------------

body {
  font: 1em/1.6em 'Open Sans', sans-serif;
  color: $main-text-color;
  border-left: 4em solid $frame-color;
  border-right: 4em solid $frame-color;
  background:  image-url('backup-bg.jpg') top center repeat-x; // Backup image for IE (doesn't load if gradient is supported, filter is no good because it's not multi-stop)
  background-color: $page-bg-color; // Old browsers
  @include background-image(linear-gradient(top, #000000 0%,#000106 4%,#03030f 7%,#04061b 12%,#06061e 17%,#09071d 20%,#0c0a20 23%,#131126 26%,#161429 29%,#20182d 32%,#22192c 35%,#2c1b2b 38%,#3a2029 41%,#3a232d 44%,#482b2d 47%,#4c302f 50%,#5b3f3c 53%,#674a44 56%,#7a5a4d 59%,#846755 62%,#9f8576 65%,#bbab9b 68%,#c6b599 71%,#b28f71 74%,#a47959 77%,#804729 80%,#642110 83%,#5d1910 86%,#51150d 89%,#40110b 92%,#2e0d08 95%,#180604 98%,#000000 100%));
  
  @include breakpoint(baby-bear) {
    border-left: none;
    border-right: none; 
  }
}

/* bottom "border" */
body:after {
  content: "";
  position: fixed;
  background: $frame-color;
  left: 0;
  right: 0;
  height: 4em;
  bottom: 0;

  @include breakpoint(baby-bear) {
    height: 0;
  }  
}

.site-wrapper {
  margin: 4em auto;
  border: 1px solid $frame-color;
  background:  image-url('stars-01.gif') fixed center center repeat-y /*, image-url('space-19.gif') fixed center center no-repeat*/;
  
  @include breakpoint(papa-bear) { 
    @include background-size(cover);
  }

  @include breakpoint(baby-bear) { 
    margin: 1em auto;
  }
}

article {
  padding-top: 15%;
  // width: 100%;
  
  @include breakpoint(mama-bear) { 
    padding-top: 30%;
  }
  @include breakpoint(baby-bear) { 
    padding-top: 50%;
  }
}

/* break */
article:last-child {
  padding-bottom: 100%;
}

h2, h3 {
  width: 50%;
  max-width: 20em;
  margin: 0 auto 2em;
  padding: 1em 2em;
  
  @include breakpoint(mama-bear) {
    width: 60%;
    padding: 0.5em 1em;
  }
}

.title-block, .pbj-title-block {
  font: 9em 'Novecento', 'Open Sans', sans-serif;
  font-weight: bold;
  color: $heading-text-color;
  text-transform: uppercase;
  text-align: center;
  width: 100%;
}

.pbj-title-block {
  position: absolute;
  letter-spacing: 0.3em;
  margin: 1.5em auto;
  @include animate(bounceIn, 1s, 3s);
}

.title-block {
  position: relative;
  font-size: 4em;
  letter-spacing: 0.1em;
  margin: 1.5em auto;
}

.pbj-super-title, .pbj-super-title:after, .super-title, .super-title:after{
  white-space: nowrap;
  display: block;
  position: relative;
  padding: 0 0.5em;
  border-bottom: solid 0.08em #FFFFFF;
  margin: auto;

  @include breakpoint(mama-bear) { 
    font-size: .5em;
    letter-spacing: 0.5em;
  }

  @include breakpoint(baby-bear) { 
    font-size: .3em;
    letter-spacing: 0.5em;
  }
}

.pbj-super-title, .pbj-super-title:after {
  width: 4em;

  @include breakpoint(mama-bear) { 
    width: 5em;
  }

  @include breakpoint(baby-bear) { 
    width: 5em;
  }
}

.super-title, .super-title:after{
  width: 6em;

  @include breakpoint(mama-bear) { 
    width: 9em;
  }

  @include breakpoint(baby-bear) { 
    width: 9em;
  }
}

.super-title {
  z-index: 0;
  text-shadow: .1em .1em 0 $main-text-color ;
  filter: progid:DXImageTransform.Microsoft.DropShadow(offX=2,offY=2,color=000000);
}

.super-title:after {
  content: attr(data-shadow-text);
  color: #ff0000;
  // color: $main-text-color ;
  border: none;
  text-shadow: none;
  position: absolute;
  left: -0.08em;
  top: -0.08em;
  z-index: -1;
  -webkit-mask-image: image-url('text-mask.png');

  @include breakpoint(mama-bear) { 
    font-size: 1em;
  }

  @include breakpoint(baby-bear) { 
    font-size: 1em;
  }
}

.super-title:last-child {
  border: none;
}

.super-title a{
  // @include unpink;
  margin: 40em auto;
}

.section-header {
  font: 1.4em/1.5em 'Novecento', 'Open Sans', sans-serif;
  color: $heading-text-color;
  border: solid $heading-text-color .3em;
  background: $heading-bg-color;
  letter-spacing: 0.1em;
  font-weight: bold;
  text-align: center;

  .debold {
    font-weight: normal;
    display: block;
  }

  @include breakpoint(mama-bear) { 
    font-size: 1em;
    line-height: 1.5em;
  }
}

.text-block {
  width: 70%;
  max-width: 45em;
  margin: auto;
  padding: 2em 3em;
  background: $main-bg-color;
  font-size: 1.1em;

  strong {
    font-weight: bold;
  }

  .special {
    background: $special-bg-color;
    font-style: italic;
    color: $special-text-color;
  }

  @include breakpoint(mama-bear) { 
    width: 75%;
    font-size: 0.9em;
    line-height: 1.4em;
    padding: 1em 1.8em;
  }
}

.subhead {
  font-weight: bold;
  font-size: 120%;
  margin-bottom: 1.5em;

  @include breakpoint(mama-bear) { 
    margin-bottom: 1em;
    font-size: 100%;
  }
}

